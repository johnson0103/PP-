int width = 100;
int height = 100;
int blockx = width / 10;
int blocky = height / 10;

void setup() {
  size(width, height);
  noLoop();
}

void draw() {
  for (int j = 0; j < 10; j ++) {
    for (int i = 0; i < 10; i ++) {
      if ((i + j + 1) % 2 == 0) {
        fill(0, 0, 0); 
      } else { 
        fill(255, 255, 255);               }
      rect(j * blockx, i * blocky, (j + 1) * blockx, (i + 1) * blocky);  
      }
    }
  }
